rootPart.CFrame = CFrame.new(173.713943, -1974.44019, -17.0912476, -0.999841332, 2.13044746e-08, 0.0178123359, 2.08730224e-08, 1, -2.44079743e-08, -0.0178123359, -2.4032305e-08, -0.999841332)
